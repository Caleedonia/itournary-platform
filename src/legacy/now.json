{
  "version": 2,
  "name": "paradise-partners-frontend",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime|css|image|media|polyfills)(?:/.*)?", "headers": { "cache-control": "public,max-age=31536000,immutable" } },
    { "src": "/static/(.*)", "headers": { "cache-control": "public,max-age=31536000,immutable" }, "dest": "/static/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
}

